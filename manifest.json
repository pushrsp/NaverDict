{
  "manifest_version": 3,
  "name": "네이버 영어사전",
  "version": "1.0.0",
  "icons": {
    "32": "./images/icon32.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "./scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": "./scripts/content.js",
      "css": "./scripts/content.css"
    }
  ]
}
